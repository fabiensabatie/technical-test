{
  "name": "@mexty/realtime",
  "version": "2.1.0",
  "description": "React hooks for real-time collaborative features using Yjs and Hocuspocus",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs",
    "dev": "rollup -c --bundleConfigAsCjs --watch",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "dependencies": {
    "yjs": "^13.6.10",
    "@hocuspocus/provider": "^3.2.3",
    "uuid": "^9.0.1",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/uuid": "^9.0.7",
    "rollup": "^4.0.0",
    "rollup-plugin-dts": "^6.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/mexty-realtime.git"
  },
  "keywords": [
    "react",
    "realtime",
    "collaborative",
    "yjs",
    "hocuspocus",
    "multiplayer"
  ],
  "author": "MEXT Team",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
